{
  "name": "ng-pack-flow",
  "version": "0.1.0",
  "description": "A workflow for Angular made with Webpack",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --history-api-fallback --hot --inline --progress",
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack --config webpack.build.js --bail -p",
    "lint": "./node_modules/.bin/eslint --config ./.eslintrc ./src/. || true",
    "test": "npm run eslint && karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umayr/ng-pack-flow.git"
  },
  "author": "Umayr Shahid <umayrr@hotmail.co.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umayr/ng-pack-flow/issues"
  },
  "homepage": "https://github.com/umayr/ng-pack-flow",
  "devDependencies": {
    "angular-mocks": "^1.4.5",
    "autoprefixer-core": "^6.0.1",
    "autoprefixer-loader": "^2.1.0",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "css-loader": "^0.16.0",
    "eslint": "^1.3.1",
    "eslint-plugin-angular": "^0.11.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.1",
    "isparta-instrumenter-loader": "^0.2.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "0.0.10",
    "node-sass": "^3.3.2",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "postcss-loader": "^0.6.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "angular": "^1.4.5",
    "angular-ui-router": "^0.2.15",
    "bootstrap-sass": "^3.3.5",
    "font-awesome": "^4.4.0"
  }
}
